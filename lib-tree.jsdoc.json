{
	"tags": {
		"allowUnknownTags": false
	},
	"plugins": [
		"plugins/markdown"
	],
	"opts": {
		"template": "node_modules/docdash/",
		"destination": "docs/",
		"readme": "README.md",
		"package": "package.json",
		"verbose": true
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	}
}